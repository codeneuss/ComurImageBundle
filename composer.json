{
    "name": "codeneuss/image-bundle",
    "type": "symfony-bundle",
    "description": "A bundle providing fields for image upload with jquery upload and image cropping with jcrop for symfony2 (forked from comour/ComurImageBundle)",
    "keywords": ["Form Type", "image", "upload", "crop", "jcrop", "jquery upload"],
    "homepage": "https://github.com/codeneuss/ImageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Can OMUR",
            "email": "omur.can@gmail.com",
            "homepage": "http://canomur.com"
        },
        {
            "name" : "Volkmar Eigler",
            "email" : "codeneuss@posteo.net",
            "homepage": "https://volkmareigler.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-gd": "*",
        "symfony/http-foundation": "~2.2|3.*",
        "symfony/form": "~2.2|3.*",
        "symfony/validator": "~2.2|3.*",
        "symfony/routing": "~2.2|3.*",
        "symfony/config": "~2.2|3.*",
        "symfony/twig-bridge": "~2.2|3.*",
        "symfony/class-loader":"~2.2|3.*",
        "twig/twig": "~1.12",
        "twig/extensions": "~1.0",
        "friendsofsymfony/jsrouting-bundle": "@stable",
	    "jms/translation-bundle": "^1.2.2"
    },
    "target-dir": "Codeneuss/ImageBundle",
    "autoload": {
        "psr-0": { "Codeneuss\\ImageBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
